<odoo>
    <record id="kota_action_view"
            model="ir.actions.act_window">
        <field name="name">Kota</field>
        <field name="res_model">wilayah.kota</field>
        <field name="view_mode">list,form,kanban</field>
    </record>

    <menuitem id="kota_menu_action" action="kota_action_view" parent="contacts.menu_localisation"/>

    <record id="city_list_view" model="ir.ui.view">
        <field name="name">wilayah.city.list</field>
        <field name="model">wilayah.kota</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
                <field name="province_code"/>
                <field name="state"/>
            </list>
        </field>
    </record>
</odoo>