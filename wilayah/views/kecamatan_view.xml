<odoo>
    <record id="kecamatan_action_view"
            model="ir.actions.act_window">
        <field name="name">Kecamatan</field>
        <field name="res_model">wilayah.kecamatan</field>
        <field name="view_mode">list,form,kanban</field>
    </record>

    <menuitem id="kecamatan_menu_action" action="kecamatan_action_view" parent="contacts.menu_localisation"/>

    <record id="district_list_view" model="ir.ui.view">
        <field name="name">wilayah.district.list</field>
        <field name="model">wilayah.kecamatan</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
                <field name="kota_id"/>
            </list>
        </field>
    </record>
</odoo>