<odoo>
    <data>

        <record id="action_estate_property_type_list" model="ir.actions.act_window">
            <field name="name">Property Types</field>
            <field name="res_model">estate.property.type</field>
            <field name="view_mode">list,form,kanban</field>
        </record>

        <menuitem id="menu_estate_property_type" name="Property Types" parent="test_first_level_menu" action="action_estate_property_type_list"/>

        <record id="view_list_property_type" model="ir.ui.view">
            <field name="name">estate.property.type.list</field>
            <field name="model">estate.property.type</field>
            <field name="arch" type="xml">
                <list string="Property Types" default_order="sequence">
                    <field name="name"/>
                </list>
            </field>
        </record>

        <record id="view_form_property_type" model="ir.ui.view">
            <field name="name">estate.property.type.form</field>
            <field name="model">estate.property.type</field>
            <field name="arch" type="xml">
                <form string="Peoperty Types">
                    <sheet>
                        <group>
                        <div class="oe_title">
                            <h1><field name="name"/></h1>
                        </div>
                            <field name="sequence"/>
                            <field name="offer_count"/>
                            <button type="action" name="%(real_estate.list_view_offer)d" string="All Offer"/>
                        </group>
                        <notebook>
                            <page string="Properties">
                                <field name="property_ids"> 
                                    <list>
                                        <field name="name" />
                                        <field name="expected_price" />
                                        <field name="state" />
                                    </list>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>